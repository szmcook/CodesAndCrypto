    "def read_basis(filename):\n",
    "    with open(filename) as f:\n",
    "        lines = [re.findall('[0-9]+', l) for l in f.readlines() if l[0] not in '#B\\n']\n",
    "    return np.array(lines, dtype=np.int64)"
